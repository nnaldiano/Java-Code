
public class BasketballPlayer extends Player {
	private String freeThrowPercentage;

	public BasketballPlayer() {
		setFreeThrowPercentage(freeThrowPercentage);
	}
	public BasketballPlayer(String freeThrowPercentage) {
		super();
		this.freeThrowPercentage = freeThrowPercentage;
	}
	
	public String getFreeThrowPercentage() {
		return freeThrowPercentage;
	}

	public void setFreeThrowPercentage(String freeThrowPercentage) {
		this.freeThrowPercentage = freeThrowPercentage;
	}
	
	public String formatPlayerinformation() {
		String Formattedplayer = this.getPlayerNumber() + this.getParticipant() + this.getPosition() + this.getFreeThrowPercentage();
	return Formattedplayer;

	}
	

}
