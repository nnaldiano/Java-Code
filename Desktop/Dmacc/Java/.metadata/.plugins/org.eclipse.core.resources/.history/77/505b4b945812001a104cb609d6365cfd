package Model;
public class TicketCounter {
	private int TICKET_AMOUNT = 45;
	private String ticketInput;	
	
	public TicketCounter() {
		
		
	}
	
	
	
	
	public int getTICKET_AMOUNT() {
		return TICKET_AMOUNT;
	}
	public void setTICKET_AMOUNT(int tICKET_AMOUNT) {
		TICKET_AMOUNT = tICKET_AMOUNT;
	}
	public String getTicketInput() {
		return ticketInput;
	}
	public void setTicketInput(String ticketInput) {
		this.ticketInput = ticketInput;
	}
	
	public boolean Check(String var) {
		boolean isInteger = false;
		try {
			Integer.parseInt(var);
			return true;
		} catch (NumberFormatException e) {
			return isInteger;
		}
	
	}
	

}
